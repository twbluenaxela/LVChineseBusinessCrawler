 jsonData={scrapedObjects} />\n    </div>\n  );\n}\n\n\nfunction DynamicTable({jsonData}) {\n  //get table columns\n  const column = Object.keys(jsonData[0]);\n\n  //table heading\n  const ThData = () => {\n    return column.map((data) => {\n      return <th key={data}>{data}</th>\n    })\n  }\n\n  //table row\n  const tdData = () => {\n    return jsonData.map((data) => {\n      return(\n        <tr>\n          {\n            column.map((v) => {\n              return <td>{data[v]}</td>\n            })\n          }\n        </tr>\n      )\n    })\n  }\n\n  return (\n    <table className=\"table\">\n      <thead>\n        <tr>{ThData()}</tr>\n      </thead>\n      <tbody>\n        {tdData()}\n      </tbody>\n    </table>\n  )\n}\n\n  export default Pachong","/workspace/LVChineseBusinessCrawler/src/constants.js",[],{"ruleId":"31","replacedBy":"32"},{"ruleId":"33","replacedBy":"34"},{"ruleId":"35","severity":1,"message":"36","line":1,"column":9,"nodeType":"37","messageId":"38","endLine":1,"endColumn":35},{"ruleId":"35","severity":1,"message":"39","line":1,"column":37,"nodeType":"37","messageId":"38","endLine":1,"endColumn":63},{"ruleId":"35","severity":1,"message":"40","line":1,"column":65,"nodeType":"37","messageId":"38","endLine":1,"endColumn":83},{"ruleId":"35","severity":1,"message":"41","line":1,"column":85,"nodeType":"37","messageId":"38","endLine":1,"endColumn":108},{"ruleId":"35","severity":1,"message":"42","line":3,"column":10,"nodeType":"37","messageId":"38","endLine":3,"endColumn":14},{"ruleId":"35","severity":1,"message":"43","line":6,"column":7,"nodeType":"37","messageId":"38","endLine":6,"endColumn":23},{"ruleId":"35","severity":1,"message":"44","line":22,"column":10,"nodeType":"37","messageId":"38","endLine":22,"endColumn":19},{"ruleId":"45","severity":1,"message":"46","line":40,"column":6,"nodeType":"47","endLine":40,"endColumn":8,"suggestions":"48"},{"ruleId":"35","severity":1,"message":"49","line":42,"column":12,"nodeType":"37","messageId":"38","endLin